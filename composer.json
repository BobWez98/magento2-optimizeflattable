{
    "name": "justbetter/magento2-optimizeflattable",
    "description": "Module for optimizing Magento flat table generation",
    "type": "magento2-module",
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "magento/framework": "*",
        "magento/module-catalog": "^104.0"
    },
    "type": "magento2-module",
    "repositories": {
        "magento": {
            "type": "composer",
            "url": "https://repo-magento-mirror.fooman.co.nz/"
        }
    },
    "authors": [
        {
            "name": "Robin Mulder",
            "email": "robin@justbetter.nl"
        }
    ],
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "JustBetter\\OptimizeFlatTables\\": ""
        }
    },
    "require-dev": {
        "bitexpert/phpstan-magento": "^0.30.1",
        "phpstan/phpstan": "^1.10"
    },
    "config": {
        "allow-plugins": {
            "magento/composer-dependency-version-audit-plugin": true
        }
    }
}
